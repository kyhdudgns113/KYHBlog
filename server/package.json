{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "server": "nest start --watch",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "gkd": "ts-node -r tsconfig-paths/register ./test/gkd.test.ts",
    "gkd1": "ts-node -r tsconfig-paths/register ./test/gkd.test.ts --LOG_LEVEL=1",
    "gkd2": "ts-node -r tsconfig-paths/register ./test/gkd.test.ts --LOG_LEVEL=2",
    "gkd3": "ts-node -r tsconfig-paths/register  ./test/gkd.test.ts --LOG_LEVEL=3",
    "gkd4": "ts-node -r tsconfig-paths/register  ./test/gkd.test.ts --LOG_LEVEL=4",
    "gkd5": "ts-node -r tsconfig-paths/register  ./test/gkd.test.ts --LOG_LEVEL=5",
    "gkd6": "ts-node -r tsconfig-paths/register  ./test/gkd.test.ts --LOG_LEVEL=6",
    "gkd7": "ts-node -r tsconfig-paths/register  ./test/gkd.test.ts --LOG_LEVEL=7",
    "gkd8": "ts-node -r tsconfig-paths/register  ./test/gkd.test.ts --LOG_LEVEL=8",
    "gkd9": "ts-node -r tsconfig-paths/register  ./test/gkd.test.ts --LOG_LEVEL=9",
    "gkd10": "ts-node -r tsconfig-paths/register  ./test/gkd.test.ts --LOG_LEVEL=10",
    "gkdDefault": "ts-node -r tsconfig-paths/register  ./test/gkd.test.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.3",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/websockets": "^11.1.3",
    "bcrypt": "^6.0.0",
    "minimist": "^1.2.8",
    "mongoose": "^8.14.3",
    "mysql2": "^3.14.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.10.7",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
